        };
        combo_excl {
            timeout-ms = <50>;
            key-positions = <26 40>;
            bindings = <&kp EXCL>;
            layers = <COLEMAK>;
        };
        combo_backslash {
            timeout-ms = <50>;
            key-positions = <25 39>;
            bindings = <&kp BACKSLASH>;
            layers = <COLEMAK SYMBOLS>;
        };
        combo_less_greater {
            timeout-ms = <50>;
            key-positions = <1 2>;
            bindings = <&lt_gt_ins>;
            layers = <SYMBOLS>;
        };
        combo_backword {
            timeout-ms = <50>;
            key-positions = <36 37>;
            bindings = <&kp LC(BSPC)>;
            layers = <COLEMAK>;
        };
        combo_escape {
            timeout-ms = <50>;
