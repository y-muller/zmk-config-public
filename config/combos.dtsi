/ {
    combos {
        compatible = "zmk,combos";
        combo_leader {
            timeout-ms = <50>;
            key-positions = <37 38>;
            bindings = <&leader>;
            layers = <COLEMAK>;
        };
        combo_adaptive_dev {
            timeout-ms = <50>;
            key-positions = <22 36>;
            bindings = <&ad_dev>;
            layers = <COLEMAK>;
        };
        combo_qu {
            timeout-ms = <50>;
            key-positions = <14 29>;
            bindings = <&macro_qu>;
            layers = <COLEMAK>;
        };
        combo_under {
            timeout-ms = <50>;
            key-positions = <23 37>;
            bindings = <&kp UNDER>;
            layers = <COLEMAK>;
        };
        combo_minus {
            timeout-ms = <50>;
            key-positions = <8 23>;
            bindings = <&minus_nobreak>;
            layers = <COLEMAK>;
        };
        combo_sqt {
            timeout-ms = <50>;
            key-positions = <9 24>;
            bindings = <&ht_sqts 0 0>;
            layers = <COLEMAK>;
        };
        combo_dqt {
            timeout-ms = <50>;
            key-positions = <10 25>;
            bindings = <&ht_dqts 0 DQT>;
